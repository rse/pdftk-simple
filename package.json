{
    "name":        "pdftk-simple",
    "version":     "1.2.8",
    "description": "Simple PDFTk Wrapper",
    "keywords":    [ "pdftk", "wrapper" ],
    "main":        "./pdftk-api.js",
    "bin": {
        "pdftk":  "./pdftk-cli.js"
    },
    "repository": {
        "type": "git",
        "url":  "https://github.com/rse/pdftk-simple.git"
    },
    "author": {
        "name":  "Dr. Ralf S. Engelschall",
        "email": "rse@engelschall.com",
        "url":   "http://engelschall.com"
    },
    "license":  "MIT",
    "homepage": "https://github.com/rse/pdftk-simple",
    "bugs":     "https://github.com/rse/pdftk-simple/issues",
    "devDependencies": {
        "babel-eslint":            "10.1.0",
        "eslint":                  "7.32.0",
        "eslint-config-standard":  "16.0.3",
        "eslint-plugin-import":    "2.25.3",
        "eslint-plugin-node":      "11.1.0",
        "tslint":                  "6.1.3",
        "typescript":              "4.5.4"
    },
    "dependencies": {
        "npm-install-fetch":       "1.3.5",
        "which":                   "2.0.2",
        "eventemitter3":           "4.0.7",
        "execa":                   "5.1.1"
    },
    "upd":                         [ "!execa", "!eslint" ],
    "engines": {
        "node":                    ">=12.0.0"
    },
    "scripts": {
        "prepublishOnly": "eslint --config eslint.yaml pdftk-*.js && tslint --config tslint.json pdftk-*.d.ts",
        "install":        "npm-install-fetch",
        "test":           "node sample.js"
    },
    "npm-install-fetch": {
        "name":    "PDFTk 3.3.2 (2021-12-22) [5.4 MB]",
        "input":   "https://gitlab.com/pdftk-java/pdftk/-/jobs/1904577721/artifacts/raw/build/libs/pdftk-all.jar",
        "output":  "pdftk-cli.jar"
    }
}
